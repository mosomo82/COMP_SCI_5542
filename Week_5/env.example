# ─────────────────────────────────────────────────────────────────────────────
# .env.example  —  Snowflake credential template
#
# Copy this file to .env and fill in your own values.
# NEVER commit .env to GitHub. Only commit this .env.example file.
# ─────────────────────────────────────────────────────────────────────────────

# Required — Snowflake account identifier (e.g. abc12345.us-east-1)
SNOWFLAKE_ACCOUNT="your_account_identifier"

# Required — Snowflake username
SNOWFLAKE_USER="your_username"

# Required (unless using SSO — see SNOWFLAKE_AUTHENTICATOR below)
SNOWFLAKE_PASSWORD="your_password"

# Required — virtual warehouse to use for queries
SNOWFLAKE_WAREHOUSE="your_warehouse_name"

# Required — database created by sql/01_create_schema.sql
SNOWFLAKE_DATABASE="CS5542_WEEK5"

# Required — schema (default: PUBLIC)
SNOWFLAKE_SCHEMA="PUBLIC"

# Optional — Snowflake role (leave blank to use your account default)
# SNOWFLAKE_ROLE="your_role_name"

# Optional — set to 'externalbrowser' to use SSO instead of a password
# SNOWFLAKE_AUTHENTICATOR="externalbrowser"
